#include <stdio.h>

int main(){
    int T=0, n=0, caso=0, i=0, k=0, f=0, q=0, kdef=0;
    scanf("%d", &T);
    for(caso=1; caso<=T; caso++){
        scanf("%d", &n);
        int v[n];
        for(i=0; i<n; i++){
            scanf("%d", &v[i]);
        }
        if(v[0]<=v[1]){  //if necessario pq o v[0] nao possui termo anterior, logo nao entra no if do proximo for
            v[0]=0;
            f++; // quant de "falso" aumenta em 1
        }
        for(i=0; i<n; i++){
            if(v[i]<=v[i+1] && v[i]>=v[i-1]){
                v[i]=0;
                if(v[n-2]==0) 
                    v[n-1]=0; //zerar o ultimo termo caso nao fa√ßa parte da seq
                f++; 
            }
        }
        k=1;
        kdef=0;
        int u[n];
        for(i=0; i<n; i++){
            u[i]=0; //zerar vetor u
        }
        for(i=1; i<n; i++){
            if(v[i-1]!=0){
                if(v[i]<v[i-1]){
                    u[k-1]=v[i-1];
                    k++;
                    printf("%d\n", u[k-1]);
                    if(i==n-1 && k>kdef){ //bloco para nao dar erro no ultimo termo da seq
                        kdef=k;
                        u[k-1]=v[n-1];
                        k=1;
                    }
                }
                else{
                    if(k>kdef){
                        kdef=k;
                        k=1;
                    }
                }
            }
        } 
        
        /*/for(i=0; i<n; i++){
            if(u[i]!=0){
                q++;
            }
        }



        printf("%d\n", q);
        for(i=0; i<n; i++){
            if(u[i]!=0){
                printf("%d ", u[i]);
            }
        }
        printf("\n");
        f=0;
        q=0;
    } /*/

    for(i=0; i<n; i++){
        printf("%d ", v[i]);
    }
    printf("\n");
    for(i=0; i<n; i++){
        printf("%d ", u[i]);
    }
    }
        
    return 0;
}
